{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "whitespace": [true, "check-module", "check-decl"],
      "interface-name": false,
      "variable-name": [true, "allow-snake-case", "allow-leading-underscore"],
      "no-console": false
    },
    "rulesDirectory": []
}